{
  "name": "OMDb",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean": "rimraf build",
    "copyassets": "copyfiles  -u 1 src/assets/*.* build/",
